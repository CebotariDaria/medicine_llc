            <Label x:Name="label32c" Content="                *Monitor- VGA sau SVGA" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,0,0,-120" FontSize="20" Foreground="#FF0C1540" />
            <Label x:Name="label33c" Content="                *spatiu pe disc 2 MB" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="0,0,0,-170" FontSize="20" Foreground="#FF0C1540" />
